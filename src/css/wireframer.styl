[wireframer]
	margin:0
	padding:0;
	list-style:none;
	*
		margin:0
		padding:0
		font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif; 
		font-weight: 300;
		border:3px solid #333;
		list-style:none;
		padding:5px;
		margin:5px;
		overflow:hidden;
	ul
		border:0
		margin 0
		padding 0
	[view]
		margin:10px;

	.iphone
		width: 372px
		height:600px
		border-radius:7px
		box-shadow: 0 0 11px black
		-webkit-transition: all 0.5s ease-in-out
		*
			-webkit-transition: all 0.5s ease-in-out
		[view]
			border-width:10px
			overflow-y:scroll
			height:480px
			width:320px
			// opacity:0
			// display:inline-block
		footer
			text-align:center;
			border:0
			.button
				margin:0 auto;
				border:0
				padding:5px;
				border-radius:100px
				height:40px
				width:40px;
				border:3px solid #333
				.button-inner
					border: 3px solid #CCC;
					margin: 7px;
					border-radius: 6px;
					width: 10px;
					height: 10px;
	nav
		button
			background:transparent
			&:nth-child(1)
				float:left;
			&:nth-child(2)
				float:right;
		button.right
			float:right;
		button.left
			float:left
	.left
		float:left;
	.right
		float:right;

	.width-50
		width:50%;

	.checkbox
		width:10px
		height:10px
		border:4px solid
	.radio
		width:10px
		height:10px
		border:4px solid
		border-radius:20px